        <OpDef>
            <Name>RoPESimple</Name>
            <Description>
                <Content>
                    LLaMA RoPE Simple
                    Only calculate:
                    value1 = in_value * cos_value - in_value_2 * sin_value and 
                    value2 = in_value * sin_value + in_value_2 * cos_value.
                </Content>
            </Description>

            <Input>
                <Name>in[0]</Name>
                <Description>
                    <Content>input activation</Content>
                </Description>
                <Mandatory>true</Mandatory>
